{
    "index_name": "docs-site",
    "start_urls": ["https://tgy.ink/"],
    "selectors": {
      "default": {
        "lvl0": ".sidebar h4.version",
        "lvl1": {
          "selector": ".sidebar-heading.active+ul li a.active",
          "strip_chars": "#"
        },
        "lvl2": {
          "selector": ".theme-hope-content h2",
          "strip_chars": "#"
        },
        "lvl3": {
          "selector": ".theme-hope-content h3",
          "strip_chars": "#"
        },
        "text": ".theme-hope-content p, .theme-hope-content li, .theme-hope-content td",
        "lang": {
          "selector": "/html/@lang",
          "type": "xpath",
          "global": true
        }
      }
    },
    "custom_settings": {
      "attributesForFaceting": [
        "lang"
      ]
    }
  }